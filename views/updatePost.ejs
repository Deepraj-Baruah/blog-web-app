<%- include("partials/header.ejs") %>
    <h1>Home</h1>
    <%= home %>
        <% database.forEach(function(post){%>
                <h1>
                    <%= post.postTitle %>
                </h1>
                <p>
                    <%= post.postContent.substring(0, 100) %>
                </p>
                <!-- <a href="/post/ <%#= post.postTitle %>">Read more</a> -->
                <a href="/post/<%= post.id %>">Read more</a>
        <%}); %> 
<%- include("partials/footer.ejs") %>